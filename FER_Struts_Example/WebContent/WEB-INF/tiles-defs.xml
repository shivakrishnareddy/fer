<?xml version="1.0" encoding="ISO-8859-1" ?>

<!DOCTYPE struts-config PUBLIC
          "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN"
          "http://struts.apache.org/dtds/struts-config_1_3.dtd">




<tiles-definitions>

	<definition name="mainLayout" path="/Layout/MainLayout.jsp">
		<put name="header" value="/Layout/Header.jsp"/>
		<put name="leftFrame" value="/Layout/LeftFrame.jsp"/>
		<put name="body" value="/Layout/Body.jsp"/>
		<put name="Footer" value="/Layout/Footer.jsp" />
	</definition>
	<definition name="dashboard" extends="mainLayout">
		
		
		<put name="body" value="/Dashboard.jsp" />
	</definition>
	<definition name="addExpense" extends="mainLayout">
		
		<put name="body" value="/AddExpense.jsp" />
	</definition>
	<definition name="editExpense" extends="mainLayout">
		
		<put name="body" value="/EditExpense.jsp" />
	</definition>
	<definition name="deleteExpense" extends="mainLayout">
		
		<put name="body" value="/DeleteExpense.jsp" />
	</definition>
	<definition name="expenseReport" extends="mainLayout">
		
		<put name="body" value="/ReportExpense.jsp" />
	</definition>

	
</tiles-definitions>
	 